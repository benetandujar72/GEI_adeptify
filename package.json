  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "NODE_ENV=development tsx server/index.ts",
    "dev:client": "vite client",
    "build": "npm run build:server && npm run build:client",
    "build:server": "node esbuild.config.js",
    "build:client": "vite build client",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "verify:db": "node scripts/verify-db.js",
    "test:db": "node scripts/test-db-simple.js",
    "test:port": "node scripts/test-port.js",
    "monitor:db": "node scripts/monitor-db-connections.js",
    "debug:startup": "node scripts/debug-startup.js",
    "debug:production": "node scripts/debug-production.js",
    "db:init": "node scripts/init-database.js",
    "db:check": "node scripts/check-database.js",
    "db:check-simple": "node scripts/check-db-simple.js",
    "diagnose": "node scripts/diagnose-production.js",
    "test:endpoints": "node scripts/test-endpoints.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "docker:build": "docker build -t gei-unified-platform .",
    "docker:run": "docker run -p 3000:3000 gei-unified-platform",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down"
  },
