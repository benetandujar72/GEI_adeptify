{
  "name": "gei-unified-platform",
  "version": "1.0.0",
  "description": "GEI Unified Platform - Ecosistema Educativo Inteligente Unificado",
  "type": "module",
  "license": "MIT",
  "workspaces": [
    "gateway",
    "microservices/*",
    "mcp/*",
    "client",
    "server"
  ],
  "scripts": {
    "install:all": "npm install",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\" \"npm:dev:gateway\"",
    "dev:server": "npm run dev --workspace=server",
    "dev:client": "npm run dev --workspace=client",
    "dev:gateway": "npm run dev --workspace=@gei/gateway",

    "build": "npm run build:server && npm run build:client",
    "build:server": "node esbuild.config.js",
    "build:client": "vite build",
    "start": "node scripts/start-render.js",
    "start:win": "node scripts/start-windows.js",
    "test:server": "node scripts/test-server-start.js",
    "test:minimal": "node scripts/test-minimal-server.js",
    "start:prod": "node scripts/start-production.js",
    "start:render": "node scripts/start-render.js",
    "verify:render": "node scripts/verify-render-deployment.js",
    "create:admin": "node scripts/create-admin-user.js",
    "test:login": "node scripts/test-login.js",
    "db:create-educational": "node scripts/create-educational-tables.js",
    "db:fix-educational": "node scripts/fix-educational-tables.js",
    "db:populate-educational": "node scripts/populate-educational-data.js",
    "check": "tsc",
    "fix:deps": "node scripts/fix-dependencies.js",
    "verify:setup": "npm run fix:deps && npm run check",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:create-tables": "node scripts/create-tables.js",
    "db:init-simple": "node scripts/init-db-simple.js",
    "db:check-simple": "node scripts/check-db-simple.js",
    "diagnose": "node scripts/diagnose.js",
    "setup:env": "node scripts/setup-env.js",
    "test:db": "node scripts/test-db-connection.js",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:all": "npm run test:unit && npm run test:integration",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "docker:build": "docker-compose build",
    "docker:run": "docker-compose up -d",
    "docker:compose:down": "docker-compose down"
  },
